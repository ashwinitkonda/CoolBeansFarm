<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="bodybg" stylesheets="@style.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ControllerWelcomeScreen">
   <children>
      <Label layoutX="23.0" layoutY="24.0" text="WELCOME TO COOL BEANS FARM!">
         <textFill>
            <RadialGradient centerX="0.5" centerY="0.5" radius="1.0">
               <stops>
                  <Stop>
                     <color>
                        <Color red="0.949999988079071" green="0.11611111462116241" blue="0.11611111462116241" />
                     </color>
                  </Stop>
                  <Stop offset="1.0">
                     <color>
                        <Color red="0.9973683953285217" green="0.9585818648338318" blue="0.9585818648338318" />
                     </color>
                  </Stop>
               </stops>
            </RadialGradient>
         </textFill>
         <font>
            <Font name="Trebuchet MS Bold Italic" size="35.0" />
         </font>
      </Label>
      <Button fx:id = "startGame" layoutX="207.0" layoutY="111.0" mnemonicParsing="false" text="START GAME" textFill="#cd2121" onAction="#handleStartGameAction">
         <font>
            <Font name="Trebuchet MS Bold Italic" size="25.0" />
         </font>
      </Button>
      <Button fx:id = "currentStats" layoutX="307.0" layoutY="176.0" mnemonicParsing="false" text="CURRENT STATS" textFill="#bd2020">
         <font>
            <Font name="Trebuchet MS Bold Italic" size="25.0" />
         </font>
      </Button>
      <Button fx:id = "resumeGame" layoutX="88.0" layoutY="176.0" mnemonicParsing="false" text="RESUME GAME" textFill="#d11e1e">
         <font>
            <Font name="Trebuchet MS Bold Italic" size="25.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
